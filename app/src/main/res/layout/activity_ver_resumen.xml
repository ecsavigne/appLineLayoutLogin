<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#202732"
    tools:context=".verResumen">

    <TextView
        android:id="@+id/id_resumen_tv_porcient_lista"
        android:layout_width="406dp"
        android:layout_height="56dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="10dp"
        android:text="@string/strTvTest"
        android:textColor="#F1F1FA"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_resumen_et_hour" />

    <TextView
        android:id="@+id/id_resumen_tv_porcient_bote"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="10dp"
        android:text="@string/strTvTest"
        android:textColor="#F1F1FA"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_resumen_tv_porcient_lista" />

    <TextView
        android:id="@+id/id_resumen_tv_miporc1"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="10dp"
        android:text="@string/strTvTest"
        android:textColor="#F1F1FA"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_resumen_tv_porcient_bote" />

    <TextView
        android:id="@+id/id_resumen_tv_total"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="10dp"
        android:text="@string/strTvTest"
        android:textColor="#F1F1FA"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_resumen_tv_miporc1" />

    <EditText
        android:id="@+id/id_resumen_et_hour"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="68dp"
        android:autofillHints=""
        android:cursorVisible="false"
        android:ems="10"
        android:focusable="false"
        android:gravity="center_horizontal"
        android:hint="@string/str_et_hora"
        android:inputType="date"
        android:maxLength="10"
        android:minHeight="48dp"
        android:textColor="#FDF9F9"
        android:textColorHint="#558EA8"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/id_resumen_et_date"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck,VisualLintTextFieldSize" />

    <EditText
        android:id="@+id/id_resumen_et_date"
        android:layout_width="159dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="68dp"
        android:autofillHints=""
        android:cursorVisible="false"
        android:ems="10"
        android:focusable="false"
        android:gravity="center_horizontal"
        android:hint="@string/str_et_fecha"
        android:inputType="date"
        android:maxLength="14"
        android:minHeight="48dp"
        android:textColor="#FDF9F9"
        android:textColorHint="#558EA8"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck,VisualLintTextFieldSize" />

    <Spinner
        android:id="@+id/id_sp_resumen_usr"
        android:layout_width="305dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="10dp"
        android:minHeight="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>